{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#FFFFFF",
          "background": "#6F6B6A",
          "properties": {
            "postfix": " ",
            "wsl_separator": " \uF105 "
          }
        },
        {
          "type": "root",
          "style": "diamond",
		  "trailing_diamond": "\uE0B0",
          "powerline_symbol": "\uE0B0",
          "background": "#F36943",
          "foreground": "#FFFFFF",
          "properties": {
            "postfix": " "
          }
        },
        {
          "type": "path",
          "style": "diamond",
          "leading_diamond": "<transparent></>",
          "trailing_diamond": "\uE0B0",
          "foreground": "#000000",
          "background": "#FFFFFF",
          "properties": {
            "home_icon": "~",
            "style": "full",
            "postfix": " ",
            "prefix": "<transparent>\uE0B0 \ue5fe </> "
          }
        },
        {
          "type": "git",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\uE0B0",
          "foreground": "#000000",
          "background": "#16C98D",
          "properties": {
			"display_status": true,
            "display_stash_count": true,
            "prefix": "<transparent>\uE0B0</> ",
            "display_upstream_icon": true,
            "status_colors_enabled": true,
            "ahead_color": "#ff0000",
            "ahead_and_behind_color": "#ff0000",
            "behind_color": "#ff0000",
            "local_changes_color": "#ffeb42",
            "branch_icon": "\uE725"
          }
        },
        {
          "type": "exit",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#2e9599",
          "leading_diamond": "",
          "trailing_diamond": "\uE0B0",
          "properties": {
            "display_exit_code": false,
            "always_enabled": false,
            "error_color": "#f1184c",
            "color_background": true,
            "prefix": "<transparent>\uE0B0</> \uf467 "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "executiontime",
          "style": "diamond",
          "leading_diamond": "\uE0B2",
          "trailing_diamond": "",
          "foreground": "#ffffff",
          "background": "#8800dd",
          "properties": {
            "threshold": 500,
            "style": "austin",
            "prefix": "<#ffffff>\ufbab </>",
            "postfix": " <transparent>\uE0b2</>"
          }
        },
        {
          "type": "dotnet",
          "style": "diamond",
          "leading_diamond": "\uE0B2",
          "trailing_diamond": "",
          "foreground": "#FFFFFF",
          "background": "#5728E2",
          "properties": {
            "display_version": false,
            "display_mode": "files",
            "prefix": " \uE70C ",
            "postfix": "<transparent>\uE0b2</>"
          }
        },
		{
          "type": "python",
          "style": "diamond",
          "leading_diamond": "\uE0B2",
          "trailing_diamond": "",
		  "foreground": "#FFDD56",
          "background": "#417FB0",
          "properties": {
            "display_mode": "files",
            "display_version": false,
            "postfix": "<transparent>\uE0b2</>",
            "prefix": " \uE235 "
          }
        },
				{
          "type": "dart",
          "style": "diamond",
          "leading_diamond": "\uE0B2",
          "trailing_diamond": "",
		  "foreground": "#03589C",
          "background": "#2BB7F6",
          "properties": {
            "display_mode": "files",
            "display_version": false,
            "postfix": "<transparent>\uE0b2</>",
            "prefix": " \uE798 "
          }
        },
        {
          "type": "node",
          "style": "diamond",
          "leading_diamond": "\uE0B2",
          "trailing_diamond": "",
          "foreground": "#ffffff",
          "background": "#6CA35E",
          "properties": {
            "display_mode": "files",
            "display_version": false,
            "postfix": "<transparent>\uE0b2</>",
            "prefix": " \uE718 "
          }
        },
        {
          "type": "time",
          "style": "diamond",
          "leading_diamond": "\uE0B2",
          "trailing_diamond": "",
          "foreground": "#000000",
          "background": "#00e1ff",
          "properties": {
            "postfix": " <#000000>\uF017</> <transparent>\uE0b2</>",
            "time_format": "15:04:05"
          }
        },
        {
          "type": "battery",
          "style": "diamond",
          "leading_diamond": "\uE0B2",
          "trailing_diamond": "",
          "foreground": "#FFFFFF",
          "background": "#ff9900",
          "properties": {
            "prefix": " ",
            "discharging_icon": "\uF582",
            "color_background": true,
            "postfix": "\uF295 ",
            "display_charging": false
          }
        }
      ]
    },
    {
      "type": "newline"
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "exit",
          "style": "plain",
          "foreground": "#58F374",
          "properties": {
            "prefix": "\ufB26",
            "always_enabled": true,
            "error_color": "#FF5252",
            "display_exit_code": false
          }
        }
      ]
    }
  ]
}

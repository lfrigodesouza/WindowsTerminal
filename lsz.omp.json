{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#FFFFFF",
          "background": "#006235",
          "properties": {
            "postfix": " "
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "background": "#ff933a",
          "foreground": "#FFFFFF",
          "properties": {
            "postfix": " "
          }
        },
        {
          "type": "path",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\uE0B0",
          "foreground": "#000000",
          "background": "#FFFFFF",
          "properties": {
            "home_icon": "~",
            "style": "full",
            "postfix": " ",
            "prefix": "<transparent>\uE0B0 \uE5FF </> "
          }
        },
        {
          "type": "git",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\uE0B0",
          "foreground": "#000000",
          "background": "#ffe600",
          "properties": {
            "display_stash_count": true,
            "prefix": "<transparent>\uE0B0</> "
          }
        },
        {
          "type": "exit",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#2e9599",
          "leading_diamond": "",
          "trailing_diamond": "\uE0B0",
          "properties": {
            "display_exit_code": false,
            "always_enabled": false,
            "error_color": "#f1184c",
            "color_background": true,
            "prefix": "<transparent>\uE0B0</> \uf467"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "executiontime",
          "style": "diamond",
          "leading_diamond": "\uE0B2",
          "trailing_diamond": "",
          "foreground": "#ffffff",
          "background": "#8800dd",
          "properties": {
            "threshold": 500,
            "style": "austin",
            "prefix": " <#ffffff>\ufbab </>",
            "postfix": " <transparent>\uE0b2</>"
          }
        },
        {
          "type": "dotnet",
          "style": "diamond",
          "leading_diamond": "\uE0B2",
          "trailing_diamond": "",
          "foreground": "#FFFFFF",
          "background": "#5728E2",
          "properties": {
            "display_version": false,
            "display_mode": "files",
            "prefix": " \uE77F ",
            "postfix": "<transparent>\uE0b2</>"
          }
        },
        {
          "type": "node",
          "style": "diamond",
          "leading_diamond": "\uE0B2",
          "trailing_diamond": "",
          "foreground": "#ffffff",
          "background": "#6CA35E",
          "properties": {
            "display_mode": "files",
            "display_version": false,
            "postfix": "<transparent>\uE0b2</>",
            "prefix": " \uE718 "
          }
        },
        {
          "type": "time",
          "style": "diamond",
          "leading_diamond": "\uE0B2",
          "trailing_diamond": "",
          "foreground": "#000000",
          "background": "#00e1ff",
          "properties": {
            "postfix": " <#000000>\uF017</> <transparent>\uE0b2</>",
            "time_format": "15:04:05"
          }
        },
        {
          "type": "battery",
          "style": "diamond",
          "leading_diamond": "\uE0B2",
          "trailing_diamond": "",
          "foreground": "#FFFFFF",
          "background": "#ff9900",
          "properties": {
            "prefix": " ",
            "discharging_icon": "\uF582",
            "color_background": true,
            "postfix": "\uF295 ",
            "display_charging": false
          }
        }
      ]
    },
    {
      "type": "newline"
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "exit",
          "style": "plain",
          "foreground": "#FFFFFF",
          "properties": {
            "prefix": "\ufB26",
            "always_enabled": true,
            "error_color": "#FF5252",
            "display_exit_code": false
          }
        }
      ]
    }
  ]
}